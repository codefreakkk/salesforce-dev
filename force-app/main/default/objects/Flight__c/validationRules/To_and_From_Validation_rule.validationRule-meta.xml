<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>To_and_From_Validation_rule</fullName>
    <active>true</active>
    <description>Validation for To and From</description>
    <errorConditionFormula>ISBLANK( To__c ) || ISBLANK( From__c )</errorConditionFormula>
    <errorMessage>To and From are required they cannot be empty</errorMessage>
</ValidationRule>
